    "003fcc50-9301-4178-835c-817b997a9825": {
      "type": "section-rich-text",
      "blocks": {
        "template--19015464255771__003fcc50-9301-4178-835c-817b997a9825-1684883819296ca298-0": {
          "type": "subheading",
          "settings": {
            "subheading": "",
            "text_size": 1
