    }
  }

  .message-button {
    margin-left: 36px;

    .btn-primary {
      font-family: 'Montserrat', sans-serif;
      font-size: 14px;
      border: 1px solid black;
      background-color: transparent;
      color: #000;
      padding: 6px 12px;
      line-height: 1.42857;
      white-space: nowrap;
      align-items: center;
      justify-content: center;
      font-weight: 500;
      text-transform: none;
      background-color: black;
      border-color: black;
      color: #fff;
      min-width: 100px;
      height: 32px;

      &:hover {
        color: black;
        background-color: #ff4d33;
      }
    }
