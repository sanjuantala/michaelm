.review-details {
  box-shadow: 0 3px 4px 0 rgba(0, 0, 0, 0.1);

  .review-items {
    display: block;
    border: thin solid #ccc;

    &:not(:first-child) {
      border-top: 0;
    }
  }

  .review-title {
    padding: 20px;
    color: #333333;
    background-color: #f7f7f7;
    border-bottom: thin solid #ccc;

    h2 {
      line-height: 32px;
      color: #333333;
      font-family: 'Montserrat', sans-serif;
      font-weight: 500;
      font-size: 18px;
      margin: 0;
    }
  }

  .review-body {
    background-color: #fff;
    padding: 20px;

    .review-inner-item {
      display: flex;
      border-top: thin solid #ddd;
      padding-top: 15px;
      padding-bottom: 15px;
      color: #333333;
      line-height: 1.42857;

      .review-small-title {
        flex-basis: 30%;
        flex-shrink: 0;
        color: #697180;
        vertical-align: top;
        font-weight: 400;
        padding-top: 5px;
        padding-bottom: 5px;
        font-size: 14px;
      }

      .review-text {
        flex-basis: 70%;
        font-weight: 500;
        word-wrap: break-word;
        font-size: 14px;
        font-weight: 500;
        padding-top: 5px;
        padding-bottom: 5px;
      }

      &:first-child {
        border: none;
        padding-top: 0;
      }

      &:last-child {
        padding-bottom: 0;
      }
    }
  }
}

.review-maps {
  margin-top: 15px;

  iframe {
    width: 100%;
    min-height: 430px;
  }
}

@media only screen and (min-width: 768px) {
  .service-list__container {
    width: 50%;
  }
}

@media only screen and (min-width: 992px) {

  .page__main-content,
  .location-section,
  .location-container {
    max-width: 940px;
  }

  .service-list__container {
    width: 33.33333%;
  }

  .card__description {
    min-height: 80px;
  }

  .details-page__form__main {
    .two-columns__main {
      padding-right: 15px;
      width: 66.66667%;
    }

    .two-columns__side {
      padding-left: 15px;
      width: 33.33333%;
    }
  }

  .review-details {
    .review-body {
      .review-inner-item {
        .review-small-title {
          width: 25%;
        }
      }
    }
  }
}

@media only screen and (max-width: 991px) {
  .page__main-content,
  .location-section,
  .location-container {
    max-width: calc(100% - 30px);
  }

  #stockist-widget#stockist-widget {
    .stockist-horizontal {
      .stockist-side-panel {
        width: 100%;
      }
    }

    .stockist-result-list {
      ul {
        margin-right: 0;
      }
    }

    .stockist-map {
      position: static;
    }
  }

  .location-section,
  .location-container {
    .page-heading__heading {
      margin-bottom: 40px;
      padding-top: 30px;
    }
  }

  .xdsoft_datetimepicker {
    &.xdsoft_inline {
      flex-direction: column;
    }

    .xdsoft_datepicker,
    .xdsoft_timepicker {
      flex-basis: 100%;
      margin: 0;
      padding-right: 0;
      width: 100%;
      border: 0;
    }

    .xdsoft_timepicker {
      padding-top: 28px;
    }
  }

  .details-page__form__main {
    .information__form {
      padding: 15px;
    }

    .checkbox-filed {
      margin-bottom: 0;

      &.form-filed {
        label {
          margin-bottom: 0;
        }
      }
    }

    .data-protection-information__form {
      .checkbox-filed {
        label {
          span {
            &:last-child {
              margin-bottom: 0;
            }
          }
        }
      }
    }

    .two-columns__side {
      display: none;
    }
  }

  .review-message {
    flex-direction: column;
    align-items: center;
    text-align: center;

    .message-button {
      margin: 15px 0 0 0;
    }
  }

  .review-details {
    .review-body {
      .review-inner-item {
        flex-direction: column;
      }
    }
  }
}

body .xdsoft_datetimepicker .xdsoft_calendar td.xdsoft_disabled {
  background: #cbcbcb !important;
  cursor: no-drop;
  pointer-events: none;
}

body .xdsoft_datetimepicker .xdsoft_calendar td.xdsoft_current:not(.xdsoft_disabled) {
  background-color: #ff4d33!important;
}

.endscreen button {
  margin: 20px auto;
}

#stockist-widget .stockist-search-filter-checkbox label {
    padding-top: 9px;
    display: inline-flex;
    align-items: self-end;
}


.engagement_datepicker.xdsoft_datetimepicker .xdsoft_datepicker {
  margin: 0;
  width: 334px;
  padding: 10px;
}

.engagement_datepicker.xdsoft_datetimepicker .xdsoft_monthpicker .xdsoft_label {
  pointer-events: auto;

  i {
    color: #000;
    display: inline-block;
    margin-right: 5px;
  }
}

.engagement_datepicker.xdsoft_datetimepicker .xdsoft_label>.xdsoft_select>div>.xdsoft_option:hover,
.engagement_datepicker.xdsoft_datetimepicker .xdsoft_calendar td:hover {
  background: #ff4d33 !important;
}

.xdsoft_time:after {
  content: 'pm';
  font-size: 12px;
}
.xdsoft_time[data-hour="11"]:after {
  content: 'am';
}